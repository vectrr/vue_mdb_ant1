touch()\r\n\r\n        if (!this.$v.$invalid) {\r\n          this.saveUser()\r\n        }\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"css\" scoped>\r\n  .md-progress-bar {\r\n    position: absolute;\r\n    top: 0;\r\n    right: 0;\r\n    left: 0;\r\n  }\r\n    .card {\r\n        max-width: 400px;\r\n        width:80%;\r\n        margin-left:auto;\r\n        margin-right:auto;\r\n    }\r\n    .mTop{\r\n        /* margin-top: 73px; */\r\n        background: linear-gradient(337deg, rgb(0 12 23) 15%, rgb(167, 202, 164) 4%, rgb(0 12 23) 90%, rgba(0, 0, 0, 0.87) 4%);\r\n        margin-top: -23%;\r\n        padding-top: 30%;\r\n    }\r\n</style>"]}]}